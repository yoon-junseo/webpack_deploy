{
    "compilerOptions": {
        "esModuleInterop": true,
        "sourceMap": true,
        "lib": [
            "ES2020",
            "DOM"
        ],
        "jsx": "react",
        "module": "esnext",
        "moduleResolution": "Node",
        "target": "es5",
        "strict": true,
        "resolveJsonModule": true,
        "baseUrl": ".",
        "paths": {
            "@components/*": [
                "components/*"
            ],
            "@containers/*": [
                "containers/*"
            ],
            "@modules/*": [
                "modules/*"
            ],
            "@pages/*": [
                "pages/*"
            ],
            "@lib/*": [
                "lib/*"
            ],
            "@typings/*": [
                "typings/*"
            ],
            "@router/*": [
                "router/*"
            ],
            "@assets/*": [
                "assets/*"
            ]
        }
    }
}